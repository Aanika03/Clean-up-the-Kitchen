Create once locally, then commit.
import torch
from model import FetchKitchenCleanupModel

model = FetchKitchenCleanupModel(state_dim=30)

torch.save(
    {
        "model_state_dict": model.state_dict(),
        "metadata": {
            "model_type": "fetch_control",
            "task": "kitchen_cleanup",
            "output_dim": 13,
            "control_mode": "pd_joint_delta_pos"
        }
    },
    "fetch_policy.pt"
)
